<?xml version="1.0" ?>

<sdf version="1.6">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <plugin name="beacons_sync_world_plugin" filename="libbeacons_sync_world_plugin.so">
      <update_rate>60</update_rate>
    </plugin>
  </world>
</sdf>